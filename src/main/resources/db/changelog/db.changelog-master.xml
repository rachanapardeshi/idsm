<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.8"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.8
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.8.xsd">


	<changeSet id="001" author="rachana">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="idsm" />
			</not>
		</preConditions>
		<sqlFile path="/db/changelog/db.changeset-001-tables.sql" />
		<comment>This change set replicates the latest DB structure</comment>
	</changeSet>

	<include file="/db/changelog/db.changelog-procedures.xml" />

</databaseChangeLog>